    <!-- è·³è½¬åŠ è½½æç¤º -->
    <div class="loading-toast" id="loadingToast">æ­£åœ¨è·³è½¬è‡³æ”¯ä»˜å®...</div>

    <script>
        // éšæœºæ•°æ®é…ç½®
        const NICKNAMES = ["å¸Œå°”è‰¾åŠ›", "å°æ˜", "å°æ", "å°å¼ ", "å°ç‹", "å°èµµ", "ç”¨æˆ·"];
        const AVATARS = [
            "https://randomuser.me/api/portraits/men/1.jpg",
            "https://randomuser.me/api/portraits/lego/1.jpg",
            "https://randomuser.me/api/portraits/thumb/men/2.jpg",
            "https://randomuser.me/api/portraits/women/1.jpg",
            "https://randomuser.me/api/portraits/thumb/women/2.jpg",
            "https://randomuser.me/api/portraits/men/3.jpg",
            "https://randomuser.me/api/portraits/women/4.jpg"
        ];
        let dynamicData = []; // å­˜å‚¨åŠ¨æ€æ•°æ®ï¼ˆå«æ—¶é—´æˆ³ï¼‰

        // æ›¿æ¢ä¸ºæ–°çš„æ”¯ä»˜å®æ”¯ä»˜é“¾æ¥
        const ALIPAY_LINK = "https://qr.alipay.com/45t14720usgwdnmonbew594";

        // ç”Ÿæˆéšæœºæç°é‡‘é¢ï¼ˆ6-36å…ƒï¼Œä¿ç•™2ä½å°æ•°ï¼‰
        function getRandomAmount() {
            return (Math.random() * 30 + 6).toFixed(2);
        }

        // æ ¹æ®æ—¶é—´æˆ³ç”Ÿæˆæ˜¾ç¤ºæ—¶é—´ï¼ˆåˆšåˆš/1-30åˆ†é’Ÿå‰ï¼‰
        function formatTime(timestamp) {
            const now = Date.now();
            const diff = Math.floor((now - timestamp) / 60000); // åˆ†é’Ÿå·®
            return diff === 0 ? "åˆšåˆš" : diff <= 30 ? `${diff}åˆ†é’Ÿå‰` : `${Math.floor(diff/60)}å°æ—¶å‰`;
        }

        // ç”Ÿæˆéšæœºç”¨æˆ·å
        function getRandomUsername() {
            const nickname = NICKNAMES[Math.floor(Math.random() * NICKNAMES.length)];
            return nickname === "ç”¨æˆ·" ? `${nickname}${Math.floor(Math.random() * 1000000)}` : nickname;
        }

        // ç”Ÿæˆå•æ¡åŠ¨æ€æ•°æ®ï¼ˆå«æ—¶é—´æˆ³ï¼‰
        function generateDynamicData() {
            return {
                id: Date.now(), // ç”¨æ—¶é—´æˆ³å½“å”¯ä¸€ID
                timestamp: Date.now() - Math.floor(Math.random() * 30 * 60000), // éšæœº0-30åˆ†é’Ÿå‰çš„æ—¶é—´æˆ³
                username: getRandomUsername(),
                amount: getRandomAmount(),
                avatar: AVATARS[Math.floor(Math.random() * AVATARS.length)]
            };
        }

        // æ¸²æŸ“åŠ¨æ€åˆ—è¡¨ï¼ˆæŒ‰æ—¶é—´æˆ³é™åºæ’åºï¼šæœ€æ–°åœ¨ä¸Šï¼‰
        function renderDynamicList() {
            const dynamicList = document.getElementById("dynamicList");
            dynamicList.innerHTML = "";
            // æ’åºåæ¸²æŸ“
            dynamicData.sort((a, b) => b.timestamp - a.timestamp).forEach(item => {
                dynamicList.innerHTML += `
                    <div class="item">
                        <div class="user-info">
                            <img src="${item.avatar}" class="user-avatar" alt="ç”¨æˆ·å¤´åƒ">
                            <div class="user-text">${item.username}å·²æç°${item.amount}å…ƒ</div>
                        </div>
                        <div class="time">${formatTime(item.timestamp)}</div>
                    </div>
                `;
            });
        }

        // åˆå§‹åŒ–ï¼šç”Ÿæˆ5æ¡éšæœºåŠ¨æ€å¹¶æ’åºæ¸²æŸ“
        function initDynamicList() {
            for (let i = 0; i < 5; i++) {
                dynamicData.push(generateDynamicData());
            }
            renderDynamicList();
        }

        // å®šæ—¶æ›´æ–°ï¼šåˆ é™¤æ—¶é—´æœ€æ—©çš„ä¸€æ¡ï¼Œæ–°å¢ä¸€æ¡ï¼Œé‡æ–°æ’åºæ¸²æŸ“ï¼ˆ3-5ç§’é—´éš”ï¼‰
        function updateDynamicList() {
            // åˆ é™¤æ—¶é—´æˆ³æœ€å°ï¼ˆæœ€æ—§ï¼‰çš„ä¸€æ¡
            dynamicData = dynamicData.sort((a, b) => a.timestamp - b.timestamp).slice(1);
            // æ–°å¢ä¸€æ¡æœ€æ–°åŠ¨æ€ï¼ˆæ—¶é—´æˆ³ä¸ºå½“å‰æ—¶é—´ï¼‰
            dynamicData.push({
                id: Date.now(),
                timestamp: Date.now(),
                username: getRandomUsername(),
                amount: getRandomAmount(),
                avatar: AVATARS[Math.floor(Math.random() * AVATARS.length)]
            });
            renderDynamicList();
            // éšæœº3-5ç§’åå†æ¬¡æ›´æ–°
            setTimeout(updateDynamicList, Math.floor(Math.random() * 2000) + 3000);
        }

        // åº•éƒ¨å¯¼èˆªåˆ‡æ¢é€»è¾‘
        const navItems = document.querySelectorAll('.nav-item');
        const pages = document.querySelectorAll('.page');

        navItems.forEach(item => {
            item.addEventListener('click', function() {
                // ç§»é™¤æ‰€æœ‰å¯¼èˆªçš„activeçŠ¶æ€
                navItems.forEach(i => i.classList.remove('active'));
                // ç»™å½“å‰å¯¼èˆªæ·»åŠ active
                this.classList.add('active');
                // éšè—æ‰€æœ‰é¡µé¢
                pages.forEach(page => page.style.display = 'none');
                // æ˜¾ç¤ºç›®æ ‡é¡µé¢
                const targetPage = document.getElementById(this.getAttribute('data-target'));
                targetPage.style.display = 'block';
            });
        });

        // æœªå®Œå–„æç¤ºå¼¹çª—é€»è¾‘
        const myOrderBtn = document.getElementById('myOrderBtn');
        const pointDetailBtn = document.getElementById('pointDetailBtn');
        const tipsMask = document.getElementById('tipsMask');
        const tipsModal = document.getElementById('tipsModal');
        const closeTipsBtn = document.querySelector('.close-tips-btn');

        // æ˜¾ç¤ºæœªå®Œå–„æç¤º
        function showTipsModal() {
            tipsMask.style.display = 'block';
            tipsModal.style.display = 'block';
        }
        myOrderBtn.addEventListener('click', showTipsModal);
        pointDetailBtn.addEventListener('click', showTipsModal);

        // å…³é—­æœªå®Œå–„æç¤º
        function closeTipsModal() {
            tipsMask.style.display = 'none';
            tipsModal.style.display = 'none';
        }
        closeTipsBtn.addEventListener('click', closeTipsModal);
        tipsMask.addEventListener('click', closeTipsModal);

        // è”ç³»å®¢æœäºŒç»´ç å¼¹çª—é€»è¾‘
        const contactServiceBtn = document.getElementById('contactServiceBtn');
        const qrcodeMask = document.getElementById('qrcodeMask');
        const qrcodeModal = document.getElementById('qrcodeModal');
        const closeQrcodeBtn = document.querySelector('.close-qrcode-btn');

        // æ˜¾ç¤ºäºŒç»´ç å¼¹çª—
        contactServiceBtn.addEventListener('click', function() {
            qrcodeMask.style.display = 'block';
            qrcodeModal.style.display = 'block';
        });

        // å…³é—­äºŒç»´ç å¼¹çª—
        function closeQrcodeModal() {
            qrcodeMask.style.display = 'none';
            qrcodeModal.style.display = 'none';
        }
        closeQrcodeBtn.addEventListener('click', closeQrcodeModal);
        qrcodeMask.addEventListener('click', closeQrcodeModal);

        // ç§¯åˆ†å…‘æ¢ç‚¹å‡»æ˜¾ç¤ºæ¿€æ´»æé†’æ¨¡å—
        const pointExchangeBtn = document.getElementById('pointExchangeBtn');
        const tipMask = document.getElementById('tipMask');
        const activateTipModal = document.getElementById('activateTipModal');

        pointExchangeBtn.addEventListener('click', function() {
            tipMask.style.display = 'block';
            activateTipModal.style.display = 'block';
        });

        // ä¼˜åŒ–åçš„æ”¯ä»˜å®æ”¯ä»˜è·³è½¬é€»è¾‘
        const alipayBtn = document.getElementById('alipayBtn');
        const loadingToast = document.getElementById('loadingToast');

        alipayBtn.addEventListener('click', function() {
            // æ˜¾ç¤ºåŠ è½½æç¤º
            loadingToast.style.display = 'block';
            // å»¶è¿Ÿè·³è½¬ï¼ˆè®©ç”¨æˆ·çœ‹åˆ°æç¤ºï¼‰
            setTimeout(() => {
                // æ–°çª—å£æ‰“å¼€æ”¯ä»˜é“¾æ¥ï¼Œé¿å…å…³é—­å½“å‰é¡µé¢
                window.open(ALIPAY_LINK, '_blank');
                // å…³é—­å¼¹çª—å’Œæç¤º
                closePayModal();
                loadingToast.style.display = 'none';
            }, 800);
        });

        // å¯åŠ¨åˆå§‹åŒ–å’Œå®šæ—¶æ›´æ–°
        initDynamicList();
        setTimeout(updateDynamicList, Math.floor(Math.random() * 2000) + 3000);

        // åŸæœ‰äº¤äº’é€»è¾‘
        const scanBarcodeBtn = document.getElementById('scanBarcodeBtn');
        const tipCancelBtn = document.getElementById('tipCancelBtn');
        const tipGoActivateBtn = document.getElementById('tipGoActivateBtn');
        const openModalBtn = document.getElementById('openModalBtn');
        const payMask = document.getElementById('payMask');
        const payModal = document.getElementById('payModal');
        const closePayModalBtn = document.getElementById('closePayModalBtn');

        scanBarcodeBtn.addEventListener('click', function() {
            tipMask.style.display = 'block';
            activateTipModal.style.display = 'block';
        });

        function closeTipModal() {
            tipMask.style.display = 'none';
            activateTipModal.style.display = 'none';
        }
        tipCancelBtn.addEventListener('click', closeTipModal);
        tipMask.addEventListener('click', closeTipModal);

        tipGoActivateBtn.addEventListener('click', function() {
            closeTipModal();
            payMask.style.display = 'block';
            payModal.style.display = 'block';
        });

        openModalBtn.addEventListener('click', function() {
            payMask.style.display = 'block';
            payModal.style.display = 'block';
        });

        function closePayModal() {
            payMask.style.display = 'none';
            payModal.style.display = 'none';
        }
        closePayModalBtn.addEventListener('click', closePayModal);
        payMask.addEventListener('click', closePayModal);

        // ç¦æ­¢æ§åˆ¶å°å¤åˆ¶ï¼ˆå¢å¼ºé˜²æŠ¤ï¼‰
        document.addEventListener('keydown', function(e) {
            // ç¦æ­¢Ctrl+Cã€Ctrl+Vã€Ctrl+Xã€Ctrl+Uã€F12
            if ((e.ctrlKey && (e.key === 'c' || e.key === 'v' || e.key === 'x' || e.key === 'u')) || e.key === 'F12') {
                e.preventDefault();
            }
        });
    </script>
